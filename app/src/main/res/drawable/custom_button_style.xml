<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="10dp"/>
            <gradient android:startColor="#CCF0E68C"
                android:endColor="#CCEEE8AA"/>
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">

            <corners android:bottomLeftRadius="6dp" android:bottomRightRadius="6dp" android:topLeftRadius="6dp" android:topRightRadius="6dp" />

            <gradient android:startColor="#fed6e3"
                android:endColor="#a8edea"
                android:angle="45"/>

            <stroke android:width="3dp"
                android:color="#cca8edea"/>

        </shape>
    </item>

</selector>